<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="x3033126.edu.gifu.u.ac.alarm_final">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Alarm_final"
        android:name=".UtilCommon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlarmListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".AlarmSettingActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".AlarmActivity"
            android:taskAffinity="alarm.wakeup"><!-- 別タスクで起動 好きなパッケージ名を指定 -->
        </activity>

        <receiver android:name=".AlarmReceiver">
        </receiver>

    </application>

</manifest>